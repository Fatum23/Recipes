{
  "expo": {
    "name": "Recipes",
    "slug": "Recipes",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/global/assets/images/app-icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/global/assets/images/app-splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "intentFilters": [
      {
        "action": "VIEW",
        "data": {
          "scheme": "https",
          "host": "example.com"
        },
        "category": ["BROWSABLE", "DEFAULT"]
      }
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/global/assets/images/app-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.fatum.recipes"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "f72b0523-bc03-45d3-85a3-36e391601089"
      }
    }
  }
}
